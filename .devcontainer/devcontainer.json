{
  "name": "Dev Container",
  "dockerFile": "Dockerfile",
  "workspaceFolder": "/app",
  "mounts": [
    {
      "source": "${localWorkspaceFolder}",
      "target": "/app",
      "type": "bind"
    }
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "be5invis.toml",
        "ms-azuretools.vscode-docker"
      ]
    }
  }
}